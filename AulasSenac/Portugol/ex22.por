programa {
    funcao inicio() {
        // Escreva um algorítimo para imrpimir os número de 1 (inclusive) a 10 (inclusive) em ordem crescente.
        escreva("\n")
        para(inteiro numero = 1; numero <= 10; numero ++) {
            escreva(numero, "\n")
        }
        escreva("\n\n")   
    }
}
