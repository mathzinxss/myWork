programa {
    funcao inicio() {
        // Escreva um algorÃ­timo para imrpimir os nÃºmero de 10 (inclusive) a 1 (inclusive) em ordem decrescente.
        escreva("\n")
        para(inteiro numero = 10; numero >= 1; numero --) {
            escreva(numero, "\n")
        }
        escreva("\n\n")   
    }
