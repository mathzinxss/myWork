programa {
    funcao inicio() {

        real num, reajuste

        escreva("Digite um valor: R$")
        leia(num)

        reajuste = num + (num * 0.05)

        escreva("O valor com reajuste de 5% é: R$", reajuste)

        escreva("\n\n")   
    }
    /*
        Faça um algoritmo que leia um valor qualquer e imprima na tela com um reajuste de 5%.
    */

}
